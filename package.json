{
  "name": "mustard-api",
  "version": "0.1.0",
  "description": "Here is the mustard",
  "engines": {
    "node": "^8.*",
    "npm": "^5.6"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint server ; exit 0",
    "dropit": "psql -c 'DROP DATABASE IF EXISTS mustard_dev' && psql -c 'CREATE DATABASE mustard_dev'",
    "restart": "yarn dropit && export NODE_ENV=development && node index.js",
    "start": " export NODE_ENV=development && node index.js",
    "pretest": "psql -c 'DROP DATABASE IF EXISTS mustard_test' && psql -c 'CREATE DATABASE mustard_test'",
    "test": "export NODE_ENV=test && node index.js",
    "test:only": "export NODE_ENV=test && ./node_modules/.bin/mocha -R spec 'test/**/**.test.js'",
    "coverage": "istanbul cover _mocha -- -R spec 'test/**/**/**/**test.js'"
  },
  "author": "Ezra Keddell",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.16.3",
    "express-jwt": "^5.1.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.9",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "supertest": "^2.0.1"
  }
}
