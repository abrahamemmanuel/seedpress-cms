{
  "name": "newseed-api",
  "version": "0.1.0",
  "description": "Here is the new seed",
  "engines": {
    "node": "^8.*",
    "npm": "^5.6"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint server ; exit 0",
    "dropit": "psql -c 'DROP DATABASE IF EXISTS newseed_dev' && psql -c 'CREATE DATABASE newseed_dev'",
    "reset": "yarn dropit && sequelize db:migrate && sequelize db:seed:all && export NODE_ENV=development && node index.js",
    "start": " export NODE_ENV=development && nodemon index.js",
    "startdb": "pg_ctl -D /usr/local/var/postgresql start",
    "stopdb": "pg_ctl -D /usr/local/var/postgresql stop",
    "pretest": "psql -c 'DROP DATABASE IF EXISTS newseed_test' && psql -c 'CREATE DATABASE newseed_test'",
    "test": "export NODE_ENV=test && node index.js",
    "test:only": "export NODE_ENV=test && ./node_modules/.bin/mocha -R spec 'test/**/**.test.js'",
    "coverage": "istanbul cover _mocha -- -R spec 'test/**/**/**/**test.js'"
  },
  "author": "Ezra Keddell",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.16.3",
    "express-jwt": "^5.1.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.9",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.8",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "faker": "^4.1.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.17.4",
    "supertest": "^2.0.1"
  }
}
